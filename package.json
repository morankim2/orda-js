{
  "name": "ortoo",
  "version": "0.0.1",
  "description": "Ortoo Javascript SDK",
  "scripts": {
    "dev": "webpack-dev-server --config=webpack.dev.conf.js",
    "test": "karma start",
    "clean": "rm -rf ./dist/*",
    "build": "npm run clean && webpack --config=webpack.dev.conf.js",
    "eslint:fix": "eslint --ext .tsx,.ts --fix ./src",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*.{json,ts,tsx}",
    "gen-model:js": "pbjs -t static-module -o src/model/model.js ./ortoo/pkg/model/*.proto",
    "gen-model:ts": "pbts -o src/model/model.d.ts -w es6 src/model/model.js",
    "gen-model": "npm run gen-model:js && npm run gen-model:ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowhunger/ortoo-js.git"
  },
  "author": "Ortoo Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/knowhunger/ortoo-js/issues"
  },
  "homepage": "https://github.com/knowhunger/ortoo-js#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.2.1",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.2.1",
    "prettier": "^2.2.0",
    "requirejs": "^2.3.6",
    "sinon": "^9.2.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "protobufjs": "^6.9.0",
    "tslog": "^3.0.1",
    "uuid": "^8.3.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.2.1"
  }
}
