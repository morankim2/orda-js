{
  "name": "ortoo",
  "version": "0.0.1",
  "description": "Ortoo Javascript SDK",
  "scripts": {
    "dev": "npx webpack serve --config=./config/webpack.dev.js",
    "test": "npx karma start",
    "coverage": "npx karma start --coverage",
    "clean": "rimraf dist coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "eslint:fix": "eslint --ext .tsx,.ts --fix ./src",
    "prettier": "prettier --config .prettierrc.json --write ./src/**/*.{json,ts,tsx}",
    "gen-openapi": "npx swagger-typescript-api -p http://192.168.1.9:29862/swagger/ortoo.grpc.swagger.json -o ./src/generated -n openapi.ts",
    "gen-proto-enum": "npx pbjs ../../ortoo-project/ortoo-js.back/ortoo/proto/ortoo.enum.proto --ts ./src/generated/proto.enum.ts && sed -i 's/export const enum/export enum/g' ./src/generated/proto.enum.ts",
    "build:dev": "npm run clean && webpack --config=./config/webpack.dev.js --stats-all --profile --bail",
    "build:prod": "npm run clean:dist && webpack --config=./config/webpack.prod.js --devtool=cheap-module-source-map --output-path=./dist --stats-all --profile --bail",
    "build:exam": "npx webpack --config=./config/webpack.prod.js --output-path=./example --devtool=inline-source-map --stats-all --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowhunger/ortoo-js.git"
  },
  "author": "Ortoo Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/knowhunger/ortoo-js/issues"
  },
  "homepage": "https://github.com/knowhunger/ortoo-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@jsdevtools/chai-exec": "^2.1.1",
    "@types/chai": "^4.2.18",
    "@types/crypto-js": "^4.0.1",
    "@types/mocha": "^8.2.2",
    "@types/paho-mqtt": "^1.0.4",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "coverage-istanbul-loader": "^3.0.5",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "eledoc": "^0.2.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "json-loader": "^0.5.7",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.1",
    "pbjs": "^0.0.14",
    "prettier": "^2.3.0",
    "requirejs": "^2.3.6",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "swagger-typescript-api": "^9.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "ws": "^7.4.5"
  },
  "dependencies": {
    "async-mutex": "^0.3.0",
    "axios": "^0.21.1",
    "core-js": "^3.12.1",
    "mqtt": "^4.2.6",
    "nanoid": "^3.1.22",
    "paho-mqtt": "^1.1.0",
    "uuid": "^8.3.1",
    "xmlhttprequest": "^1.8.0"
  }
}
