{
  "name": "ortoo",
  "version": "0.0.1",
  "description": "Ortoo Javascript SDK",
  "scripts": {
    "dev": "npx webpack serve --config=./config/webpack.dev.js",
    "test": "npx karma start",
    "coverage": "npx karma start --coverage",
    "clean": "rimraf dist coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "eslint:fix": "eslint --ext .tsx,.ts --fix ./src",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*.{json,ts,tsx}",
    "gen-openapi": "npx openapi-typescript http://127.0.0.1:29861/swagger/ortoo.grpc.swagger.json --output ./src/generated/openapi.ts && npx eslint --fix ./src/generated/openapi.ts",
    "gen-protobuf": "npx pbjs ../ortoo-js.back/ortoo/proto/ortoo.enum.proto --ts ./src/generated/proto.ts && npx eslint --fix ./src/generated/proto.ts && sed -i 's/export const enum/export enum/g' src/generated/proto.ts",
    "build:dev": "npm run clean && webpack --config=./config/webpack.dev.js --stats-all --profile --bail",
    "build:prod": "npm run clean:dist && webpack --config=./config/webpack.prod.js --devtool=cheap-module-source-map --output-path=./dist --stats-all --profile --bail",
    "build:exam": "npx webpack --config=./config/webpack.prod.js --output-path=./example --devtool=inline-source-map --stats-all --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowhunger/ortoo-js.git"
  },
  "author": "Ortoo Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/knowhunger/ortoo-js/issues"
  },
  "homepage": "https://github.com/knowhunger/ortoo-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@jsdevtools/chai-exec": "^2.1.1",
    "@types/chai": "^4.2.15",
    "@types/crypto-js": "^4.0.1",
    "@types/mocha": "^8.2.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.0",
    "coverage-istanbul-loader": "^3.0.5",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "eledoc": "^0.2.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jsdoc": "^3.6.6",
    "json-loader": "^0.5.7",
    "karma": "^6.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.2.1",
    "openapi-typescript": "^3.0.1",
    "pbjs": "^0.0.14",
    "prettier": "^2.2.1",
    "requirejs": "^2.3.6",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "terser-webpack-plugin": "^5.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.23",
    "typescript": "^4.1.2",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "async-mutex": "^0.3.0",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "mqtt": "^4.2.6",
    "uuid": "^8.3.1",
    "xmlhttprequest": "^1.8.0"
  }
}
